generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Agendamento {
  id        String   @id @default(uuid())
  nome      String
  servico   String
  data      String
  hora      String
  stats     Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
